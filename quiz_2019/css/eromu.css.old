@font-face
{
    font-family: "main";
   	font-weight: normal;
   	src: url("../g/OpenSans-Regular.ttf");
}
@font-face
{
    font-family: "main";
   	font-weight: bold;
   	src: url("../g/OpenSans-Bold.ttf");
}
body
{	
	font-family: "main", serif;
	font-size: 12px;
	line-height: 14px;		
	color: #000;
	overflow: hidden;
	background-color: #dfdabd;
}
a
{
	text-decoration: none;
	color: #000;
}
a:hover
{
	text-decoration: none;
	color: #444;
	cursor: pointer;
}
input, textarea, select
{
	font-family: "main", serif;
}
.unselectable
{
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	-o-user-select: none;
	user-select: none;	
}
.main
{
	margin: 0px 0px 0px 0px;
	padding: 0px;
	width: 1920px;	
	height: 1080px;
	margin: auto;
	position: absolute;
	left: 0px;
	top: 0px;		
}

#page_start
{	
	text-align: center;
	font-size: 40px;
	line-height: 45px;
	font-weight: bold;
	color: #46932d;
	text-transform: uppercase;
}
#page_start img
{	
	display: inline-block;
	margin-bottom: 30px;
}
#page_start .hu_btn
{	
	position: absolute;
	width: 300px;
	left: 560px;
	top: 350px;
	cursor: pointer;
}
#page_start .en_btn
{	
	position: absolute;
	width: 300px;
	left: 1060px;
	top: 350px;
	cursor: pointer;
}

#page_test
{	
	display: none;
}
#page_test .question
{
	position: absolute;	
	left: 121px;
	top: 40px;
	width: 1150px;	
	font-size: 60px;	font-weight: bold;	line-height: 70px;	color: #46932d;}
#page_test .answer
{
	position: absolute;	
	width: 450px;
	height: 450px;
	border-radius: 225px;
	border: 5px dotted transparent;
	background-color: transparent;
	cursor: pointer;
}
#page_test .answer .right
{
	position: absolute;
	left: 350px;
	top: 30px;
	width: 112px;
	height: 119px;
	background-image: url( "../g/right.png" );
}
#page_test .answer .wrong
{
	position: absolute;
	left: 355px;
	top: 30px;
	width: 162px;
	height: 118px;
	background-image: url( "../g/wrong.png" );
}
#page_test .answer table
{
	width: 100%;
	height: 100%;
}
#page_test .answer td
{	
	text-align: center;
	font-size: 50px;
	color: #597cad;
	line-height: 50px;
	font-weight: bold;
	vertical-align: middle;	
}
#page_test .answer td h1
{		
	font-weight: bold;
	font-size: 110px;
	line-height: 115px;
	margin: 0px;
	padding: 0px;
}
#page_test .answer td h2
{		
	font-weight: bold;
	font-size: 90px;
	line-height: 95px;
	margin: 0px;
	padding: 0px;
}
#page_test .answer td h3
{		
	font-weight: bold;
	font-size: 70px;
	line-height: 75px;
	margin: 0px;
	padding: 0px;
}
#page_test .answerpos1
{
	left: 147px;
	top: 402px;
}
#page_test .answerpos2
{
	left: 714px;
	top: 402px;
}
#page_test .answerpos3
{
	left: 1294px;
	top: 402px;
}

#page_drag
{	
	display: none;
}
#page_drag .question
{
	position: absolute;	
	left: 121px;
	top: 40px;
	width: 1150px;
}#page_drag .question h1{	font-size: 60px;	font-weight: bold;	line-height: 70px;	color: #46932d;	margin: 0px;}#page_drag .question h2{	font-size: 40px;	font-weight: bold;	line-height: 45px;	color: #46932d;	margin: 0px;}
#page_drag .answers
{
	position: absolute;	
	left: 0px;
	top: 0px;
	width: 1920px;
	height: 1080px;	
}
#page_drag .answers .answer
{
	position: absolute;	
	text-align: center;
}
#page_drag .answers .answer .label_under
{
	margin: 10px 0px 0px 0px;
	padding: 0px;
	font-weight: bold;
	font-size: 20px;
	line-height: 22px;
	color: #46932d;	
	text-shadow: 0px 0px 3px #fff;
}
#page_drag .answers .answer .label_inner
{
	max-width: 253px;
	margin: -115px 0px 0px 0px;
	padding: 0px;
	font-weight: bold;
	font-size: 22px;
	line-height: 24px;
	color: #fff;	
	text-shadow: 0px 0px 5px #000;
}
#page_drag .answers .answer .label_inner_1
{
	max-width: 253px;
	margin: 0px 0px 0px 0px;
	padding: 0px;
	font-weight: bold;
	font-size: 20px;
	line-height: 24px;
	color: #fff;	
	text-shadow: 0px 0px 5px #000;
}
#page_drag .answers .answer .label_left
{
	position: absolute;
	left: -310px;
	top: 17px;			
	padding: 0px;
	width: 300px;
	font-weight: bold;
	font-size: 20px;
	line-height: 22px;
	color: #46932d;	
	text-align: right;
	text-shadow: 0px 0px 3px #fff;
}
#page_drag .answers .answer .label_right
{
	position: absolute;
	left: 190px;
	top: 17px;	
	padding: 0px;
	width: 300px;
	font-weight: bold;
	font-size: 20px;
	line-height: 22px;
	color: #46932d;	
	text-align: left;
	text-shadow: 0px 0px 3px #fff;
}
#page_drag .answers .answer img
{
	user-drag: none; 
	user-select: none;
	-moz-user-select: none;
	-webkit-user-drag: none;
	-webkit-user-select: none;
	-ms-user-select: none;		
}
#page_drag .answers .answer .right
{
	position: absolute;
	left: 190px;
	top: 30px;
	width: 60px;
	height: 64px;
	background-image: url( "../g/right_small.png" );
}
#page_drag .answers .answer .wrong
{
	position: absolute;
	left: 195px;
	top: 30px;
	width: 80px;
	height: 58px;
	background-image: url( "../g/wrong_small.png" );
}
#page_drag .extras .extra
{
	position: absolute;
	font-weight: bold;
	font-size: 24px;
	line-height: 28px;
	color: #46932d;
}
#page_drag .extras .extra h1
{		
	font-weight: bold;
	font-size: 80px;
	line-height: 85px;
	margin: 0px;
	padding: 0px;
}
#page_drag .extras .extra h2
{		
	font-weight: bold;
	font-size: 60px;
	line-height: 65px;
	margin: 0px;
	padding: 0px;
}
#page_drag .extras .extra h3
{		
	font-weight: bold;
	font-size: 40px;
	line-height: 45px;
	margin: 0px;
	padding: 0px;
}


